{
  "name": "main",
  "version": "0.1.0",
  "main": "dist/main.js",
  "scripts": {
    "compile": "tsc",
    "watch": "yarn compile -w",
    "dev:web": "lerna run --scope renderer start",
    "dev:electron": "electron ./dist/main.js",
    "pkg": "electron-builder"
  },
  "devDependencies": {
    "electron": "10.2.0",
    "electron-builder": "^22.9.1",
    "typescript": "^4.1.3",
    "@types/node": "^12.19.12"
  },
  "build": {
    "appId": "com.rxliuli.electron_example",
    "productName": "electron 示例应用",
    "directories": {
      "output": "release"
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  }
}
