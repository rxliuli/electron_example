{
  "name": "main",
  "version": "0.1.0",
  "main": "dist/main.js",
  "scripts": {
    "compile": "tsc",
    "watch": "yarn compile -w",
    "dev:web": "lerna run --scope renderer start",
    "dev:electron": "electron ./dist/main.js",
    "build:web": "lerna run --scope renderer build",
    "pkg": "gulp clean && yarn compile && yarn build:web && gulp copyStatic && electron-builder"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.6",
    "@types/gulp": "^4.0.7",
    "@types/node": "^12.19.12",
    "electron": "10.2.0",
    "electron-builder": "^22.9.1",
    "fs-extra": "^9.0.1",
    "gulp": "^4.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "yaml": "^1.10.0"
  },
  "build": {
    "appId": "com.rxliuli.electron_example",
    "productName": "electron 示例应用",
    "directories": {
      "output": "release"
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  }
}
